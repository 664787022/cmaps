   "source": [
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "import cmaps\n",
    "import numpy as np\n",
    "\n",
    "x = y = np.arange(-3.0, 3.01, 0.05)\n",
    "X, Y = np.meshgrid(x, y)\n",
    "Z1 = plt.mlab.bivariate_normal(X, Y, 1.0, 1.0, 0.0, 0.0)\n",
    "plt.pcolormesh(X,Y,Z1,cmap=cmaps.NMCRef1)\n",
    "plt.colorbar()\n"
   ]
  }
